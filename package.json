    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1",
      "^@/(.*)$": "<rootDir>/$1"
    },
    "testTimeout": 10000,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
